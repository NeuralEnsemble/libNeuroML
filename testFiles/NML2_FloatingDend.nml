<?xml version="1.0" encoding="UTF-8"?>

<neuroml xmlns="http://www.neuroml.org/schema/neuroml2"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xi="http://www.w3.org/2001/XInclude"
      xsi:schemaLocation="http://www.neuroml.org/schema/neuroml2 http://neuroml.svn.sourceforge.net/viewvc/neuroml/NeuroML2/Schemas/NeuroML2/NeuroML_v2alpha.xsd"
    id="NML2_FloatingDend">
        
    <notes> Example of a multicompartmental cell with "floating dendrites" </notes>
    
    <cell id="NML2_FloatingDend">

        <morphology id="FloatingDend_morphology">

            <segment id ="0" name="Soma">
                <proximal x="0" y="0" z="0" diameter="10"/>
                <distal x="0" y="10" z="0" diameter="10"/>
            </segment>

            <segment id ="1" name="Dendrite1">
                <parent segment="0"/>
                <proximal x="20" y="5" z="0" diameter="1"/>
                <distal x="30" y="5" z="0" diameter="1"/>
            </segment>

            <segment id ="2" name="Dendrite2">
                <parent segment="0"/>
                <proximal x="-20" y="5" z="0" diameter="1"/>
                <distal x="-30" y="5" z="0" diameter="1"/>
            </segment>
            
        </morphology>

    </cell>

</neuroml>
